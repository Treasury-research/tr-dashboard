{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES5",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es6"
    ],
    "types": ["lodash", "dayjs"],
    "resolveJsonModule": true,
    "isolatedViews": true,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext"
  },
  "include": ["src"]
}
